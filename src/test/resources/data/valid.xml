<?xml version="1.0" encoding="UTF-8"?>
<papers xmlns:tns ="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.example.org/papers"
         tns:schemaLocation="http://www.example.org/papers ../schema/schema.xsd">

    <not-glossy-paper id="n1">
        <title>Example Not Glossy Paper</title>
        <number-of-pages>10</number-of-pages>
        <price>4</price>
        <monthly>true</monthly>
        <color>true</color>
        <publication-date>--02-25</publication-date>
        <not-glossy-paper-type>NEWSPAPER</not-glossy-paper-type>
    </not-glossy-paper>

    <glossy-paper id="b1" note="for everyone">
        <title>Example Glossy Paper</title>
        <number-of-pages>3</number-of-pages>
        <price>2</price>
        <monthly>false</monthly>
        <color>true</color>
        <publication-date>--02-25</publication-date>
        <glossy-paper-type>BOOKLET</glossy-paper-type>
    </glossy-paper>






</papers>

